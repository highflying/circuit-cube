  "license": "MIT",
  "dependencies": {
    "@abandonware/noble": "1.9.2-15",
    "debug": "^4.3.3"
  },
  "devDependencies": {
    "@types/debug": "4.1.7",
    "@types/node": "^16",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint": "8.35.0",
    "prettier": "2.8.4",
    "dts-bundle": "0.7.3"
  }
